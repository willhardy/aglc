%%%% Print the name of the month
\def\PrintMonth #1{%
	\ifcase #1\relax
		\or
		January\or
		February\or
		March\or
		April\or
		May\or
		June\or
		July\or
		August\or
		September\or
		October\or
		November\or
		December
	\fi
}
\def\PrintCurrentMonth{\PrintMonth{\month}}
\def\Today{\the\day{} \PrintCurrentMonth{} \the\year}

